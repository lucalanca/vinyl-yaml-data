{
  "name": "vinyl-yaml-data",
  "version": "0.0.0",
  "description": "Convert vinyl objects of YAML files into plane objects",
  "repository": "shinnn/vinyl-yaml-data",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/vinyl-yaml-data/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "gulpfriendly",
    "yaml",
    "yml",
    "vinyl",
    "object",
    "property",
    "path",
    "name",
    "filename",
    "stream",
    "convert",
    "transform"
  ],
  "dependencies": {
    "bufferstreams": "0.0.2",
    "js-yaml": "^3.2.4",
    "object-path": "^0.8.1",
    "replace-ext": "0.0.1",
    "through2": "^0.6.3",
    "try-stream-push": "^1.1.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^0.10.2",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.9.0",
    "simple-bufferstream": "0.0.4",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3",
    "vinyl": "^0.4.6"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
